{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "/components/*"
      ],
      "screens/*": [
        "/screens/*"
      ],
      "navigation/*": [
        "/navigation/*"
      ],
      "media/*": [
        "/media/*"
      ],
    },
    "jsx": "react-native",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "**/*.test.tsx"
  ]
}