{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint -c .eslintrc.json . --fix --ext .js,.jsx,.ts,.tsx",
    "cypress:ci": "expo start --web & wait-on http-get://localhost:19006 && cypress run",
    "stop": "pkill -f expo-cli"
  },
  "jest": {
    "projects": [
      {
        "preset": "jest-expo/android",
        "testPathIgnorePatterns": [
          "<rootDir>/cypress/integration/"
        ]
      },
      {
        "preset": "jest-expo/ios",
        "testPathIgnorePatterns": [
          "<rootDir>/cypress/integration/"
        ]
      }
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^0.17.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.15",
    "@react-navigation/material-bottom-tabs": "^6.2.4",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "@react-navigation/web": "~1.0.0-alpha.9",
    "expo": "^46.0.9",
    "expo-app-loading": "~2.1.0",
    "expo-asset": "~8.6.1",
    "expo-av": "~12.0.4",
    "expo-constants": "~13.2.3",
    "expo-font": "~10.2.0",
    "expo-web-browser": "~11.0.0",
    "jest": "^28.1.3",
    "node-fetch": "^3.2.10",
    "nth-check": "^2.1.1",
    "react": "18.0.0",
    "react-native": "0.69.6",
    "react-native-countdown-circle-timer": "^3.0.9",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~2.7.0",
    "react-native-paper": "^4.12.5",
    "react-native-reanimated": "~2.10.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "12.4.4",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.9",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-native": "^4.0.11",
    "@testing-library/react-native": "^11.0.0",
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.39.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-preset-expo": "~9.2.0",
    "cypress": "^10.6.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest-expo": "^46.0.1",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "react-dom": "18.0.0",
    "typescript": "~4.8.4",
    "wait-on": "^6.0.1"
  },
  "private": true
}
