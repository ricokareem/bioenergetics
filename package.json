{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint -c .eslintrc.json . --fix --ext .js,.jsx,.ts,.tsx",
    "cypress:ci": "expo start:web --localhost --port 19006 & wait-on http-get://localhost:19006 && cypress run",
    "stop": "pkill -f expo-cli"
  },
  "jest": {
    "projects": [
      {
        "preset": "jest-expo/android",
        "testPathIgnorePatterns": [
          "<rootDir>/cypress/integration/"
        ]
      },
      {
        "preset": "jest-expo/ios",
        "testPathIgnorePatterns": [
          "<rootDir>/cypress/integration/"
        ]
      }
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.15",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "@react-navigation/web": "~1.0.0-alpha.9",
    "expo": "^44.0.6",
    "expo-app-loading": "~1.3.0",
    "expo-asset": "~8.4.3",
    "expo-av": "~10.2.1",
    "expo-constants": "~13.0.1",
    "expo-font": "~10.0.3",
    "expo-web-browser": "~10.1.1",
    "jest": "^26.6.3",
    "node-fetch": "^3.2.3",
    "nth-check": "^2.0.1",
    "react": "17.0.1",
    "react-native": "0.64.3",
    "react-native-countdown-circle-timer": "^3.0.9",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~2.4.1",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "3.4.1",
    "react-native-screens": "~3.13.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react-native": "^9.2.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.21.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-preset-expo": "9.0.2",
    "cypress": "^9.5.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "jest-expo": "^44.0.1",
    "jest-junit": "^13.2.0",
    "prettier": "^2.6.2",
    "react-dom": "17.0.1",
    "typescript": "~4.6.3",
    "wait-on": "^6.0.1"
  },
  "private": true
}
